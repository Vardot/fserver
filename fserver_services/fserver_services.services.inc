<?php

/**
 * @file
 * Bulk export of services objects generated by Bulk export module.
 */

/**
 * Implements hook_default_services_endpoint().
 */
function fserver_services_default_services_endpoint() {
  $endpoints = array();

  $endpoint = new stdClass();
  $endpoint->disabled = FALSE; /* Edit this to true to make a default endpoint disabled initially */
  $endpoint->api_version = 3;
  $endpoint->name = 'fserver_release_history';
  $endpoint->server = 'rest_server';
  $endpoint->path = 'fserver';
  $endpoint->authentication = array();
  $endpoint->server_settings = array(
    'formatters' => array(
      'x' => TRUE,
      'xml' => TRUE,
      'bencode' => FALSE,
      'json' => FALSE,
      'jsonp' => FALSE,
      'php' => FALSE,
      'rss' => FALSE,
    ),
    'parsers' => array(
      'application/xml' => TRUE,
      'text/xml' => TRUE,
      'application/json' => FALSE,
      'application/vnd.php.serialized' => FALSE,
      'application/x-www-form-urlencoded' => FALSE,
      'multipart/form-data' => FALSE,
    ),
  );
  $endpoint->resources = array(
    'project' => array(
      'relationships' => array(
        'release-history' => array(
          'enabled' => '1',
        ),
      ),
    ),
  );
  $endpoint->debug = 0;
  $endpoints['fserver_release_history'] = $endpoint;

  return $endpoints;
}
